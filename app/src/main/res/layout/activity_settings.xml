                    android:layout_marginTop="@dimen/_19pxv"
                    android:layout_weight="0.84"
                    android:layout_width="0dp" android:singleLine="true"
                    android:text="@{settingsVM.settingsModel.txtEditpaymentme}"
                    style="@style/txtLexendregular15_1" tools:text="@string/msg_edit_payment_me"/>
                <ImageView android:id="@+id/imageArrowOne"
                    android:layout_height="@dimen/_2pxh"
                    android:layout_marginBottom="@dimen/_24pxv"
                    android:layout_marginEnd="@dimen/_27pxh"
                    android:layout_marginStart="@dimen/_68pxh"
                    android:layout_marginTop="@dimen/_26pxv"
                    android:layout_width="@dimen/_31pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_arrow1_red_300"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_arrow1_red_300"/>
            </LinearLayout>
            <LinearLayout android:id="@+id/linearRowaddemailaddre"
