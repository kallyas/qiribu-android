                        android:layout_width="match_parent" android:orientation="horizontal">
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtLanguage"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="@dimen/_1pxv"
                            android:layout_marginTop="@dimen/_4pxv"
                            android:layout_weight="0.86"
                            android:layout_width="0dp"
                            android:lineSpacingExtra="@dimen/_1pxh"
                            android:singleLine="true"
                            android:text="@{settingsVM.settingsModel.txtLanguage}"
                            style="@style/txtLexendregular15_1" tools:text="@string/msg_hi_there_jeffe"/>
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtSeven"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="@dimen/_5pxh"
                            android:layout_weight="0.14"
                            android:layout_width="0dp"
                            android:lineSpacingExtra="@dimen/_1pxh"
                            android:singleLine="true"
                            android:text="@{settingsVM.settingsModel.txtSeven}"
                            style="@style/txtLexendregular20" tools:text="@string/lbl"/>
