                        android:layout_gravity="start"
                        android:layout_height="@dimen/_72pxh"
                        android:layout_width="match_parent"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_rectangle"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_rectangle"/>
                    <LinearLayout android:id="@+id/linearRowpiffjenkins"
                        android:layout_gravity="bottom|center"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_21pxv"
                        android:layout_marginEnd="@dimen/_19pxh"
                        android:layout_marginStart="@dimen/_19pxh"
                        android:layout_marginTop="@dimen/_21pxv"
                        android:layout_width="match_parent" android:orientation="horizontal">
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtPiffJenkins"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="@dimen/_6pxv"
                            android:layout_marginTop="@dimen/_2pxv"
                            android:layout_weight="0.41"
                            android:layout_width="0dp"
                            android:singleLine="true"
                            android:text="@{settingsVM.settingsModel.txtPiffJenkins}"
                            style="@style/txtLexendmedium14" tools:text="@string/lbl_jeffery_bezos"/>
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtPrice"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="@dimen/_50pxh"
                            android:layout_weight="0.59"
                            android:layout_width="0dp"
                            android:singleLine="true"
                            android:text="@{settingsVM.settingsModel.txtPrice}"
                            style="@style/txtLexendmedium22" tools:text="@string/lbl_44_516_13_ugx"/>
                    </LinearLayout>
                </FrameLayout>
                <androidx.appcompat.widget.Toolbar
                    android:id="@+id/toolbarToolbar"
                    android:layout_gravity="top|start"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_39pxv"
                    android:layout_marginEnd="@dimen/_14pxh"
                    android:layout_marginStart="@dimen/_14pxh"
                    android:layout_marginTop="@dimen/_39pxv"
