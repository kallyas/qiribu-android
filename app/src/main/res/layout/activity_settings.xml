                    android:layout_width="@dimen/_18pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_vector_red_300"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_vector_red_300"/>
            </LinearLayout>
            <LinearLayout android:id="@+id/linearRowlogout"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_18pxh"
                android:layout_marginStart="@dimen/_7pxh"
                android:layout_marginTop="@dimen/_10pxv"
                android:layout_width="match_parent"
                android:orientation="horizontal" style="@style/groupStyleblue_50cornerRadius">
                <TextView android:ellipsize="end"
