                android:layout_marginStart="@dimen/_7pxh"
                android:layout_marginTop="@dimen/_10pxv"
                android:layout_width="match_parent"
                android:orientation="horizontal" style="@style/groupStyleblue_50cornerRadius">
                <TextView android:ellipsize="end"
                    android:id="@+id/txtAddProfilepho"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_17pxv"
                    android:layout_marginStart="@dimen/_28pxh"
                    android:layout_marginTop="@dimen/_19pxv"
                    android:layout_weight="0.8"
                    android:layout_width="0dp" android:singleLine="true"
                    android:text="@{settingsVM.settingsModel.txtAddProfilepho}"
                    style="@style/txtLexendregular15_1" tools:text="@string/msg_add_profile_pho"/>
                <ImageView android:id="@+id/imageArrowOneTwo"
                    android:layout_height="@dimen/_2pxh"
                    android:layout_marginBottom="@dimen/_24pxv"
                    android:layout_marginEnd="@dimen/_27pxh"
                    android:layout_marginStart="@dimen/_100pxh"
                    android:layout_marginTop="@dimen/_26pxv"
                    android:layout_width="@dimen/_31pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_arrow1_red_300"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_arrow1_red_300"/>
            </LinearLayout>
            <LinearLayout android:id="@+id/linearRoweditpassword"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_18pxh"
                android:layout_marginStart="@dimen/_7pxh"
                android:layout_marginTop="@dimen/_10pxv"
                android:layout_width="match_parent"
                android:orientation="horizontal" style="@style/groupStyleblue_50cornerRadius">
                <TextView android:ellipsize="end"
                    android:id="@+id/txtEditPassword"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_19pxv"
                    android:layout_marginStart="@dimen/_28pxh"
                    android:layout_marginTop="@dimen/_17pxv"
