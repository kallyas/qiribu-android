                    android:layout_weight="0.65"
                    android:layout_width="0dp" android:singleLine="true"
                    android:text="@{settingsVM.settingsModel.txtLogout}"
                    style="@style/txtLexendregular15_1" tools:text="@string/lbl_log_out"/>
                <ImageView android:id="@+id/imageArrowOneFour"
                    android:layout_height="@dimen/_2pxh"
                    android:layout_marginBottom="@dimen/_24pxv"
                    android:layout_marginEnd="@dimen/_27pxh"
                    android:layout_marginStart="@dimen/_169pxh"
                    android:layout_marginTop="@dimen/_26pxv"
                    android:layout_width="@dimen/_31pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_arrow1_red_300_2x31"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_arrow1_red_300_2x31"/>
            </LinearLayout>
        </LinearLayout>
        <FrameLayout android:gravity="start"
            android:id="@+id/frameBottombar"
            android:layout_height="wrap_content"
            android:layout_width="match_parent" style="@style/groupStylegray_51">
            <LinearLayout android:id="@+id/linearRowvectorOne"
                android:layout_height="match_parent"
                android:layout_marginBottom="@dimen/_32pxv"
                android:layout_marginTop="@dimen/_21pxv"
                android:layout_width="match_parent" android:orientation="horizontal">
                <ImageView android:id="@+id/imageVectorOne"
                    android:layout_gravity="center"
                    android:layout_height="@dimen/_26pxh"
                    android:layout_marginBottom="@dimen/_1pxv"
                    android:layout_marginStart="@dimen/_42pxh"
                    android:layout_marginTop="@dimen/_1pxv"
                    android:layout_width="@dimen/_26pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_vector_deep_orange_100"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_vector_deep_orange_100"/>
                <ImageView android:id="@+id/imageDiscount"
                    android:layout_gravity="center"
                    android:layout_height="@dimen/_24pxh"
                    android:layout_marginBottom="@dimen/_1pxv"
                    android:layout_marginStart="@dimen/_62pxh"
                    android:layout_marginTop="@dimen/_1pxv"
                    android:layout_width="@dimen/_25pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_discount_deep_orange_100"
