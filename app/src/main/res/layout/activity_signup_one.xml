<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="signupOneVM" type="com.qiribu.app.modules.signupone.data.viewmodel.SignupOneVM"/>
    </data>
    <FrameLayout android:id="@+id/frameSignupOne"
        android:layout_height="match_parent"
        android:layout_width="match_parent" style="@style/groupStylewhite_A700">
        <LinearLayout android:id="@+id/linearColumncreateaqiribu"
            android:layout_gravity="bottom|center"
            android:layout_height="match_parent"
            android:layout_marginBottom="@dimen/_20pxv"
            android:layout_marginEnd="@dimen/_31pxh"
            android:layout_marginStart="@dimen/_31pxh"
            android:layout_marginTop="@dimen/_20pxv"
            android:layout_width="match_parent" android:orientation="vertical">
            <androidx.appcompat.widget.Toolbar
                android:gravity="center_horizontal"
                android:id="@+id/toolbarToolbar"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_5pxh"
                android:layout_marginStart="@dimen/_6pxh"
                android:layout_width="match_parent" app:contentInsetStart="0dp">
                <LinearLayout android:id="@+id/linearRowcreateaqiribu"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <TextView android:id="@+id/txtCreateaQiribu"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_8pxv"
                        android:layout_marginTop="@dimen/_8pxv"
                        android:layout_weight="0.74"
                        android:layout_width="0dp"
                        android:lineSpacingExtra="@dimen/_3pxh"
                        android:text="@{signupOneVM.signupOneModel.txtCreateaQiribu}"
                        style="@style/txtLexendmedium22_1" tools:text="@string/msg_create_a_qiribu"/>
                    <ImageView android:id="@+id/imageQiribulogobla"
                        android:layout_height="@dimen/_60pxh"
                        android:layout_marginStart="@dimen/_75pxh"
                        android:layout_width="@dimen/_60pxh"
                        android:scaleType="fitXY"
                        imageRes="@{@drawable/img_qiribulogobla}"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_qiribulogobla"/>
                </LinearLayout>
            </androidx.appcompat.widget.Toolbar>
            <EditText
                android:background="@drawable/rectangle_bg_gray_50_radius_10"
                android:ellipsize="end"
                android:hint="@string/lbl_jeffery"
                android:id="@+id/etGroup10598" android:inputType="text"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_33pxv"
                android:layout_width="match_parent"
                android:paddingBottom="@dimen/_17pxv"
                android:paddingEnd="@dimen/_30pxh"
                android:paddingStart="@dimen/_28pxh"
                android:paddingTop="@dimen/_19pxv"
                android:singleLine="true"
                android:text="@={signupOneVM.signupOneModel.etGroup10598Value}"
                android:textColorHint="@color/bluegray_700"
                style="@style/etSolidRounded_2" tools:ignore="Autofill"/>
            <EditText
                android:background="@drawable/rectangle_bg_gray_50_radius_10"
                android:ellipsize="end" android:hint="@string/lbl_bezos"
                android:id="@+id/etGroup10600" android:inputType="text"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_10pxv"
                android:layout_width="match_parent"
                android:paddingBottom="@dimen/_18pxv"
                android:paddingEnd="@dimen/_30pxh"
                android:paddingStart="@dimen/_28pxh"
                android:paddingTop="@dimen/_18pxv"
                android:singleLine="true"
                android:text="@={signupOneVM.signupOneModel.etGroup10600Value}"
                android:textColorHint="@color/bluegray_700"
                style="@style/etSolidRounded_2" tools:ignore="Autofill"/>
            <EditText
                android:background="@drawable/rectangle_bg_gray_50_radius_10"
                android:ellipsize="end"
                android:hint="@string/lbl_256_940003000"
                android:id="@+id/etGroup10601" android:inputType="text"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_10pxv"
                android:layout_width="match_parent"
                android:paddingBottom="@dimen/_18pxv"
                android:paddingEnd="@dimen/_30pxh"
                android:paddingStart="@dimen/_28pxh"
                android:paddingTop="@dimen/_18pxv"
                android:singleLine="true"
                android:text="@={signupOneVM.signupOneModel.etGroup10601Value}"
                android:textColorHint="@color/bluegray_700"
                style="@style/etSolidRounded_2" tools:ignore="Autofill"/>
            <EditText
                android:background="@drawable/rectangle_bg_gray_50_radius_10"
                android:ellipsize="end"
                android:hint="@string/msg_jeffbezos_amazo"
                android:id="@+id/etEmail"
                android:inputType="textEmailAddress"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_10pxv"
                android:layout_width="match_parent"
                android:paddingBottom="@dimen/_17pxv"
                android:paddingEnd="@dimen/_30pxh"
                android:paddingStart="@dimen/_28pxh"
                android:paddingTop="@dimen/_19pxv"
                android:singleLine="true"
                android:text="@={signupOneVM.signupOneModel.etEmailValue}"
                android:textColorHint="@color/bluegray_700"
                style="@style/etSolidRounded_2" tools:ignore="Autofill"/>
            <LinearLayout android:id="@+id/linearRowpassword"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_10pxv"
                android:layout_width="match_parent"
                android:orientation="horizontal" style="@style/groupStylegray_50cornerRadius">
                <TextView android:ellipsize="end"
                    android:id="@+id/txtPassword"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_13pxv"
                    android:layout_marginStart="@dimen/_28pxh"
                    android:layout_marginTop="@dimen/_13pxv"
                    android:layout_weight="0.83"
                    android:layout_width="0dp" android:singleLine="true"
                    android:text="@{signupOneVM.signupOneModel.txtPassword}"
                    style="@style/txtLexendregular25" tools:text="@string/lbl2"/>
                <ImageView android:id="@+id/imageEye"
                    android:layout_height="@dimen/_14pxh"
                    android:layout_marginBottom="@dimen/_19pxv"
                    android:layout_marginEnd="@dimen/_20pxh"
                    android:layout_marginStart="@dimen/_126pxh"
                    android:layout_marginTop="@dimen/_19pxv"
                    android:layout_width="@dimen/_22pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_eye"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_eye"/>
            </LinearLayout>
            <EditText
                android:background="@drawable/rectangle_bg_gray_50_radius_10"
                android:drawableEnd="@drawable/img_vector"
                android:drawablePadding="@dimen/_30pxh"
                android:ellipsize="end"
                android:hint="@string/lbl_subalunaba_1"
                android:id="@+id/etGroup10604" android:inputType="text"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_10pxv"
                android:layout_width="match_parent"
                android:paddingBottom="@dimen/_18pxv"
                android:paddingEnd="@dimen/_20pxh"
                android:paddingStart="@dimen/_28pxh"
                android:paddingTop="@dimen/_18pxv"
                android:singleLine="true"
                android:text="@={signupOneVM.signupOneModel.etGroup10604Value}"
                android:textColorHint="@color/bluegray_700"
                style="@style/etSolidRounded_2" tools:ignore="Autofill"/>
            <TextView android:ellipsize="end"
                android:id="@+id/txtOrSignupwith"
                android:layout_gravity="center_horizontal"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_54pxh"
                android:layout_marginStart="@dimen/_54pxh"
                android:layout_marginTop="@dimen/_40pxv"
                android:layout_width="wrap_content"
                android:singleLine="true"
                android:text="@{signupOneVM.signupOneModel.txtOrSignupwith}"
                style="@style/txtLexendregular14" tools:text="@string/lbl_or_sign_up_with"/>
            <ImageView android:id="@+id/imageFrame10552"
                android:layout_gravity="center_horizontal"
                android:layout_height="@dimen/_52pxh"
                android:layout_marginEnd="@dimen/_53pxh"
                android:layout_marginStart="@dimen/_54pxh"
                android:layout_marginTop="@dimen/_17pxv"
                android:layout_width="@dimen/_206pxh"
                android:scaleType="fitXY"
                android:src="@drawable/img_frame10552"
                tools:ignore="ContentDescription" tools:src="@drawable/img_frame10552"/>
            <androidx.appcompat.widget.AppCompatButton
                android:ellipsize="end" android:gravity="center"
                android:id="@+id/btnCreateAccount"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_38pxv"
                android:layout_width="match_parent"
                android:paddingBottom="@dimen/_18pxv"
                android:paddingEnd="@dimen/_30pxh"
                android:paddingStart="@dimen/_30pxh"
                android:paddingTop="@dimen/_18pxv"
                android:singleLine="true"
                android:text="@string/lbl_create_account"
                android:textAllCaps="false"
                style="@style/btnSolidRounded" tools:text="@string/lbl_create_account"/>
        </LinearLayout>
    </FrameLayout>
</layout>
