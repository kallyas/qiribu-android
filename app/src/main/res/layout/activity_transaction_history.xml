<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="transactionHistoryVM" type="com.qiribu.app.modules.transactionhistory.data.viewmodel.TransactionHistoryVM"/>
    </data>
    <LinearLayout android:id="@+id/linearTransactionhis"
        android:layout_height="match_parent"
        android:layout_marginBottom="@dimen/_1pxv"
        android:layout_marginTop="@dimen/_38pxv"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylewhite_A700">
        <LinearLayout android:gravity="start"
            android:id="@+id/linearColumnrectangle"
            android:layout_height="0dp"
            android:layout_marginEnd="@dimen/_12pxh"
            android:layout_marginStart="@dimen/_24pxh"
            android:layout_weight="1.0"
            android:layout_width="match_parent" android:orientation="vertical">
            <FrameLayout android:id="@+id/frameStackrectangle"
                android:layout_height="@dimen/_158pxv" android:layout_width="@dimen/_338pxh">
                <FrameLayout android:id="@+id/frameCreditCard"
                    android:layout_gravity="bottom|center"
                    android:layout_height="@dimen/_72pxv"
                    android:layout_marginEnd="@dimen/_10pxh"
                    android:layout_marginTop="@dimen/_10pxv" android:layout_width="@dimen/_326pxh">
                    <ImageView android:id="@+id/imageRectangle"
                        android:layout_gravity="start"
                        android:layout_height="@dimen/_72pxh"
                        android:layout_width="match_parent"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_rectangle"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_rectangle"/>
                    <LinearLayout android:id="@+id/linearRowpiffjenkins"
                        android:layout_gravity="bottom|center"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_21pxv"
                        android:layout_marginEnd="@dimen/_19pxh"
                        android:layout_marginStart="@dimen/_19pxh"
                        android:layout_marginTop="@dimen/_21pxv"
                        android:layout_width="match_parent" android:orientation="horizontal">
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtPiffJenkins"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="@dimen/_6pxv"
                            android:layout_marginTop="@dimen/_2pxv"
                            android:layout_weight="0.41"
                            android:layout_width="0dp"
                            android:singleLine="true"
                            android:text="@{transactionHistoryVM.transactionHistoryModel.txtPiffJenkins}"
                            style="@style/txtLexendmedium14" tools:text="@string/lbl_jeffery_bezos"/>
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtPrice"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="@dimen/_50pxh"
                            android:layout_weight="0.59"
                            android:layout_width="0dp"
                            android:singleLine="true"
                            android:text="@{transactionHistoryVM.transactionHistoryModel.txtPrice}"
                            style="@style/txtLexendmedium22" tools:text="@string/lbl_44_516_13_ugx"/>
                    </LinearLayout>
                </FrameLayout>
                <androidx.appcompat.widget.Toolbar
                    android:id="@+id/toolbarToolbar"
                    android:layout_gravity="top|start"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_39pxv"
                    android:layout_marginEnd="@dimen/_14pxh"
                    android:layout_marginStart="@dimen/_14pxh"
                    android:layout_marginTop="@dimen/_39pxv"
                    android:layout_width="match_parent" app:contentInsetStart="0dp">
                    <LinearLayout android:id="@+id/linearRowlanguage"
                        android:layout_height="wrap_content"
                        android:layout_width="match_parent" android:orientation="horizontal">
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtLanguage"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="@dimen/_1pxv"
                            android:layout_marginTop="@dimen/_4pxv"
                            android:layout_weight="0.86"
                            android:layout_width="0dp"
                            android:lineSpacingExtra="@dimen/_1pxh"
                            android:singleLine="true"
                            android:text="@{transactionHistoryVM.transactionHistoryModel.txtLanguage}"
                            style="@style/txtLexendregular15_1" tools:text="@string/msg_hi_there_jeffe"/>
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtTwo"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="@dimen/_5pxh"
                            android:layout_weight="0.14"
                            android:layout_width="0dp"
                            android:lineSpacingExtra="@dimen/_1pxh"
                            android:singleLine="true"
                            android:text="@{transactionHistoryVM.transactionHistoryModel.txtTwo}"
                            style="@style/txtLexendregular20" tools:text="@string/lbl"/>
                    </LinearLayout>
                </androidx.appcompat.widget.Toolbar>
                <ImageView android:id="@+id/imageUser"
                    android:layout_gravity="top|end"
                    android:layout_height="@dimen/_87pxh"
                    android:layout_marginBottom="@dimen/_10pxv"
                    android:layout_marginStart="@dimen/_10pxh"
                    android:layout_width="@dimen/_87pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_user_87x87"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_user_87x87"/>
            </FrameLayout>
            <Spinner
                android:background="@drawable/layer_list_bg_blue_50_radius_10"
                android:gravity="end"
                android:id="@+id/spinnerGroup10777"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_12pxh"
                android:layout_marginStart="@dimen/_12pxh"
                android:layout_marginTop="@dimen/_21pxv"
                android:layout_width="match_parent" tools:listitem="@layout/spinner_item"/>
            <LinearLayout android:gravity="center_horizontal"
                android:id="@+id/linearProtein"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_12pxh"
                android:layout_marginTop="@dimen/_7pxv"
                android:layout_width="match_parent"
                android:orientation="vertical" style="@style/groupStylewhite_1">
                <LinearLayout android:id="@+id/linearRowprotein"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_11pxh"
                    android:layout_marginStart="@dimen/_11pxh"
                    android:layout_marginTop="@dimen/_15pxv"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtProtein"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_1pxv"
                        android:layout_weight="0.67"
                        android:layout_width="0dp"
                        android:singleLine="true"
                        android:text="@{transactionHistoryVM.transactionHistoryModel.txtProtein}"
                        style="@style/txtLexendregular14" tools:text="@string/lbl_cash_out"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtEightyTwo"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_211pxh"
                        android:layout_marginTop="@dimen/_1pxv"
                        android:layout_weight="0.33"
                        android:layout_width="0dp"
                        android:singleLine="true"
                        android:text="@{transactionHistoryVM.transactionHistoryModel.txtEightyTwo}"
                        style="@style/txtLexendsemibold14" tools:text="@string/lbl_80"/>
                </LinearLayout>
                <ProgressBar android:gravity="center_horizontal"
                    android:id="@+id/progressBarLine"
                    android:indeterminate="false"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_14pxv"
                    android:layout_marginEnd="@dimen/_11pxh"
                    android:layout_marginStart="@dimen/_11pxh"
                    android:layout_marginTop="@dimen/_12pxv"
                    android:layout_width="match_parent"
                    android:max="100" android:minHeight="@dimen/_6pxv"
                    android:progress="84"
                    android:progressDrawable="@drawable/layer_list_bg_gray_52_radius_3" style="@style/Widget.AppCompat.ProgressBar.Horizontal"/>
            </LinearLayout>
            <androidx.recyclerview.widget.RecyclerView
                android:gravity="center_horizontal"
                android:id="@+id/recyclerListellipsesix"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_12pxh"
                android:layout_marginTop="@dimen/_36pxv"
                android:layout_width="match_parent"
                android:orientation="vertical"
                app:layoutManager="androidx.recyclerview.widget.LinearLayoutManager"
                itemSpace="@{@dimen/_23pxh}" tools:itemCount="6" tools:listitem="@layout/row_listellipsesix"/>
        </LinearLayout>
        <FrameLayout android:gravity="start"
            android:id="@+id/frameBottombar"
            android:layout_height="wrap_content"
            android:layout_width="match_parent" style="@style/groupStylegray_51">
            <LinearLayout android:id="@+id/linearRowvector"
                android:layout_height="match_parent"
                android:layout_marginBottom="@dimen/_32pxv"
                android:layout_marginTop="@dimen/_21pxv"
                android:layout_width="match_parent" android:orientation="horizontal">
                <ImageView android:id="@+id/imageVector"
                    android:layout_gravity="center"
                    android:layout_height="@dimen/_26pxh"
                    android:layout_marginBottom="@dimen/_1pxv"
                    android:layout_marginStart="@dimen/_42pxh"
                    android:layout_marginTop="@dimen/_1pxv"
                    android:layout_width="@dimen/_26pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_vector_deep_orange_100"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_vector_deep_orange_100"/>
                <ImageView android:id="@+id/imageDiscount"
                    android:layout_gravity="center"
                    android:layout_height="@dimen/_24pxh"
                    android:layout_marginBottom="@dimen/_1pxv"
                    android:layout_marginStart="@dimen/_62pxh"
                    android:layout_marginTop="@dimen/_1pxv"
                    android:layout_width="@dimen/_25pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_discount_deep_orange_100"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_discount_deep_orange_100"/>
                <ImageView android:id="@+id/imagePiechart"
                    android:layout_gravity="center"
                    android:layout_height="@dimen/_26pxh"
                    android:layout_marginBottom="@dimen/_1pxv"
                    android:layout_marginStart="@dimen/_62pxh"
                    android:layout_marginTop="@dimen/_1pxv"
                    android:layout_width="@dimen/_26pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_piechart_red_300"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_piechart_red_300"/>
                <ImageView android:id="@+id/imageVectorOne"
                    android:layout_gravity="center"
                    android:layout_height="@dimen/_26pxh"
                    android:layout_marginBottom="@dimen/_1pxv"
                    android:layout_marginEnd="@dimen/_39pxh"
                    android:layout_marginStart="@dimen/_62pxh"
                    android:layout_marginTop="@dimen/_1pxv"
                    android:layout_width="@dimen/_26pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_vector_deep_orange_100_26x26"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_vector_deep_orange_100_26x26"/>
            </LinearLayout>
        </FrameLayout>
    </LinearLayout>
</layout>
