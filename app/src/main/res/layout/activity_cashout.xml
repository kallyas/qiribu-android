<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="cashoutVM" type="com.qiribu.app.modules.cashout.data.viewmodel.CashoutVM"/>
    </data>
    <FrameLayout android:id="@+id/frameCashout"
        android:layout_height="match_parent"
        android:layout_marginBottom="@dimen/_1pxv"
        android:layout_marginTop="@dimen/_38pxv"
        android:layout_width="match_parent" style="@style/groupStylewhite_A700">
        <LinearLayout android:id="@+id/linearColumnrectangle"
            android:layout_gravity="top|end"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/_10pxv"
            android:layout_marginStart="@dimen/_10pxh"
            android:layout_width="match_parent" android:orientation="vertical">
            <FrameLayout android:id="@+id/frameStackrectangle"
                android:layout_gravity="center_horizontal"
                android:layout_height="@dimen/_158pxv"
                android:layout_marginEnd="@dimen/_12pxh" android:layout_width="@dimen/_338pxh">
                <FrameLayout android:id="@+id/frameCreditCard"
                    android:layout_gravity="bottom|center"
                    android:layout_height="@dimen/_72pxv"
                    android:layout_marginEnd="@dimen/_10pxh"
                    android:layout_marginTop="@dimen/_10pxv" android:layout_width="@dimen/_326pxh">
                    <ImageView android:id="@+id/imageRectangle"
                        android:layout_gravity="start"
                        android:layout_height="@dimen/_72pxh"
                        android:layout_width="match_parent"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_rectangle"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_rectangle"/>
                    <LinearLayout android:id="@+id/linearRowpiffjenkins"
                        android:layout_gravity="bottom|center"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_21pxv"
                        android:layout_marginEnd="@dimen/_19pxh"
                        android:layout_marginStart="@dimen/_19pxh"
                        android:layout_marginTop="@dimen/_21pxv"
                        android:layout_width="match_parent" android:orientation="horizontal">
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtPiffJenkins"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="@dimen/_6pxv"
                            android:layout_marginTop="@dimen/_2pxv"
                            android:layout_weight="0.41"
                            android:layout_width="0dp"
                            android:singleLine="true"
                            android:text="@{cashoutVM.cashoutModel.txtPiffJenkins}"
                            style="@style/txtLexendmedium14" tools:text="@string/lbl_jeffery_bezos"/>
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtPrice"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="@dimen/_50pxh"
                            android:layout_weight="0.59"
                            android:layout_width="0dp"
                            android:singleLine="true"
                            android:text="@{cashoutVM.cashoutModel.txtPrice}"
                            style="@style/txtLexendmedium22" tools:text="@string/lbl_44_516_13_ugx"/>
                    </LinearLayout>
                </FrameLayout>
                <androidx.appcompat.widget.Toolbar
                    android:id="@+id/toolbarToolbar"
                    android:layout_gravity="top|start"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_39pxv"
                    android:layout_marginEnd="@dimen/_14pxh"
                    android:layout_marginStart="@dimen/_14pxh"
                    android:layout_marginTop="@dimen/_39pxv"
                    android:layout_width="match_parent" app:contentInsetStart="0dp">
                    <LinearLayout android:id="@+id/linearRowlanguage"
                        android:layout_height="wrap_content"
                        android:layout_width="match_parent" android:orientation="horizontal">
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtLanguage"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="@dimen/_1pxv"
                            android:layout_marginTop="@dimen/_4pxv"
                            android:layout_weight="0.86"
                            android:layout_width="0dp"
                            android:lineSpacingExtra="@dimen/_1pxh"
                            android:singleLine="true"
                            android:text="@{cashoutVM.cashoutModel.txtLanguage}"
                            style="@style/txtLexendregular15_1" tools:text="@string/msg_hi_there_jeffe"/>
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtOne"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="@dimen/_5pxh"
                            android:layout_weight="0.14"
                            android:layout_width="0dp"
                            android:lineSpacingExtra="@dimen/_1pxh"
                            android:singleLine="true"
                            android:text="@{cashoutVM.cashoutModel.txtOne}"
                            style="@style/txtLexendregular20" tools:text="@string/lbl"/>
                    </LinearLayout>
                </androidx.appcompat.widget.Toolbar>
                <ImageView android:id="@+id/imageUser"
                    android:layout_gravity="top|end"
                    android:layout_height="@dimen/_87pxh"
                    android:layout_marginBottom="@dimen/_10pxv"
                    android:layout_marginStart="@dimen/_10pxh"
                    android:layout_width="@dimen/_87pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_user"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_user"/>
            </FrameLayout>
            <TextView android:ellipsize="end"
                android:id="@+id/txtDaysuntilPayd"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_10pxh"
                android:layout_marginTop="@dimen/_37pxv"
                android:layout_width="match_parent"
                android:singleLine="true"
                android:text="@{cashoutVM.cashoutModel.txtDaysuntilPayd}"
                style="@style/txtLexendregular15_1" tools:text="@string/msg_days_until_payd"/>
            <EditText
                android:background="@drawable/rectangle_bg_gray_50_radius_10"
                android:ellipsize="end"
                android:hint="@string/lbl_enter_amount"
                android:id="@+id/etTxtCashOutAmount"
                android:inputType="text"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_10pxh"
                android:layout_marginStart="@dimen/_7pxh"
                android:layout_marginTop="@dimen/_15pxv"
                android:layout_width="match_parent"
                android:paddingBottom="@dimen/_18pxv"
                android:paddingEnd="@dimen/_30pxh"
                android:paddingStart="@dimen/_28pxh"
                android:paddingTop="@dimen/_18pxv"
                android:singleLine="true"
                android:text="@={cashoutVM.cashoutModel.etTxtCashOutAmountValue}"
                android:textColorHint="@color/red_300"
                style="@style/etSolidRounded" tools:ignore="Autofill"/>
            <TextView android:ellipsize="end"
                android:id="@+id/txtOruseslider"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_122pxh"
                android:layout_marginStart="@dimen/_122pxh"
                android:layout_marginTop="@dimen/_13pxv"
                android:layout_width="wrap_content"
                android:singleLine="true"
                android:text="@{cashoutVM.cashoutModel.txtOruseslider}"
                style="@style/txtLexendmedium13" tools:text="@string/lbl_or_use_slider"/>
            <LinearLayout android:gravity="start"
                android:id="@+id/linearRowpriceOne"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_10pxh"
                android:layout_marginStart="@dimen/_1pxh"
                android:layout_marginTop="@dimen/_23pxv"
                android:layout_width="match_parent" android:orientation="horizontal">
                <TextView android:ellipsize="end"
                    android:id="@+id/txtPriceOne"
                    android:layout_height="wrap_content"
                    android:layout_weight="0.31"
                    android:layout_width="0dp" android:singleLine="true"
                    android:text="@{cashoutVM.cashoutModel.txtPriceOne}"
                    style="@style/txtLexendmedium10" tools:text="@string/lbl_0_ugx"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtPriceTwo"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_223pxh"
                    android:layout_weight="0.69"
                    android:layout_width="0dp" android:singleLine="true"
                    android:text="@{cashoutVM.cashoutModel.txtPriceTwo}"
                    style="@style/txtLexendmedium10" tools:text="@string/lbl_44_516_13_ugx"/>
            </LinearLayout>
            <ImageView android:id="@+id/imageItem"
                android:layout_height="@dimen/_10pxh"
                android:layout_marginEnd="@dimen/_10pxh"
                android:layout_marginStart="@dimen/_1pxh"
                android:layout_marginTop="@dimen/_5pxv"
                android:layout_width="match_parent"
                android:scaleType="fitXY"
                android:src="@drawable/img_item"
                tools:ignore="ContentDescription" tools:src="@drawable/img_item"/>
            <EditText
                android:background="@drawable/rectangle_bg_blue_50_radius_16_5"
                android:ellipsize="end"
                android:hint="@string/lbl_44_000_ugx"
                android:id="@+id/etPriceThree" android:inputType="text"
                android:layout_gravity="end"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_25pxh"
                android:layout_marginStart="@dimen/_25pxh"
                android:layout_marginTop="@dimen/_6pxv"
                android:layout_width="wrap_content"
                android:paddingBottom="@dimen/_6pxv"
                android:paddingEnd="@dimen/_15pxh"
                android:paddingStart="@dimen/_16pxh"
                android:paddingTop="@dimen/_11pxv"
                android:singleLine="true"
                android:text="@={cashoutVM.cashoutModel.etPriceThreeValue}"
                android:textColorHint="@color/bluegray_700"
                style="@style/etSolidRounded_1" tools:ignore="Autofill"/>
            <TextView android:ellipsize="end"
                android:id="@+id/txtSelectacasho"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_10pxh"
                android:layout_marginTop="@dimen/_34pxv"
                android:layout_width="match_parent"
                android:singleLine="true"
                android:text="@{cashoutVM.cashoutModel.txtSelectacasho}"
                style="@style/txtLexendregular15_1" tools:text="@string/msg_select_a_cash_o"/>
            <androidx.recyclerview.widget.RecyclerView
                android:id="@+id/recyclerRowvector"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_17pxv"
                android:layout_width="match_parent"
                android:orientation="horizontal"
                app:layoutManager="androidx.recyclerview.widget.LinearLayoutManager"
                itemSpace="@{@dimen/_10pxh}" tools:itemCount="2" tools:listitem="@layout/row_rowvector"/>
            <Spinner
                android:background="@drawable/layer_list_bg_blue_50_radius_10"
                android:id="@+id/spinnerGroup10777"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_10pxh"
                android:layout_marginTop="@dimen/_24pxv"
                android:layout_width="match_parent" tools:listitem="@layout/spinner_item"/>
        </LinearLayout>
        <FrameLayout android:id="@+id/frameBottombar"
            android:layout_gravity="bottom|start"
            android:layout_height="wrap_content"
            android:layout_width="wrap_content" style="@style/groupStylegray_51">
            <LinearLayout android:id="@+id/linearRowvectorThree"
                android:layout_height="match_parent"
                android:layout_width="match_parent" android:orientation="horizontal">
                <ImageView android:id="@+id/imageVectorThree"
                    android:layout_gravity="center"
                    android:layout_height="@dimen/_26pxh"
                    android:layout_marginBottom="@dimen/_1pxv"
                    android:layout_marginStart="@dimen/_42pxh"
                    android:layout_marginTop="@dimen/_1pxv"
                    android:layout_width="@dimen/_26pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_vector_deep_orange_100"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_vector_deep_orange_100"/>
                <ImageView android:id="@+id/imageDiscount"
                    android:layout_gravity="center"
                    android:layout_height="@dimen/_24pxh"
                    android:layout_marginBottom="@dimen/_1pxv"
                    android:layout_marginStart="@dimen/_62pxh"
                    android:layout_marginTop="@dimen/_1pxv"
                    android:layout_width="@dimen/_25pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_discount"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_discount"/>
                <ImageView android:id="@+id/imagePiechart"
                    android:layout_gravity="center"
                    android:layout_height="@dimen/_26pxh"
                    android:layout_marginBottom="@dimen/_1pxv"
                    android:layout_marginStart="@dimen/_62pxh"
                    android:layout_marginTop="@dimen/_1pxv"
                    android:layout_width="@dimen/_26pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_piechart"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_piechart"/>
                <ImageView android:id="@+id/imageVectorFour"
                    android:layout_gravity="center"
                    android:layout_height="@dimen/_26pxh"
                    android:layout_marginBottom="@dimen/_1pxv"
                    android:layout_marginEnd="@dimen/_39pxh"
                    android:layout_marginStart="@dimen/_62pxh"
                    android:layout_marginTop="@dimen/_1pxv"
                    android:layout_width="@dimen/_26pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_vector_deep_orange_100_26x26"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_vector_deep_orange_100_26x26"/>
            </LinearLayout>
        </FrameLayout>
    </FrameLayout>
</layout>
